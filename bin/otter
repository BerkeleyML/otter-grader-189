#!/usr/bin/env python3

###############################
##### Otter-Grader Binary #####
###############################

from otter.argparser import get_parser

parser = get_parser()

if __name__ == "__main__":
	args = parser.parse_args()
	if hasattr(args, 'func'):
		args.func(args)
	else:
		print("You must specify a command:\n    assign\n    check\n    generate\n    grade\n	service")